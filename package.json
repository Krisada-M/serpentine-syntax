{
  "name": "serpentine-syntax",
  "displayName": "Poison Serpent",
  "description": "custom by Krisada",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.84.0"
  },
  "publisher": "Krisada-3131",
  "categories": [
    "Themes"
  ],
  "contributes": {
    "configurationDefaults": {
      "code-runner.runInTerminal": true,
      "diffEditor.ignoreTrimWhitespace": false,
      "editor.acceptSuggestionOnEnter": "on",
      "editor.accessibilitySupport": "off",
      "editor.bracketPairColorization.enabled": true,
      "editor.cursorBlinking": "expand",
      "editor.cursorWidth": 3,
      "editor.fontFamily": "'JetBrains Mono Light', 'Bai Jamjuree', 'Fira Code'",
      "editor.fontLigatures": true,
      "editor.fontSize": 16,
      "editor.fontWeight": "500",
      "editor.formatOnSave": false,
      "editor.guides.bracketPairs": "active",
      "editor.guides.bracketPairsHorizontal": true,
      "editor.guides.highlightActiveIndentation": "always",
      "editor.inlineSuggest.enabled": true,
      "editor.snippetSuggestions": "top",
      "editor.stickyScroll.enabled": true,
      "editor.suggest.preview": true,
      "editor.suggest.showMethods": true,
      "editor.suggestSelection": "first",
      "editor.wordWrap": "on",
      "explorer.confirmDelete": false,
      "functionTemplate": "// ${func_name} :: This function is used to   .",
      "typeTemplate": "// ${type_name} This struct used .",
      "git-graph.graph.style": "angular",
      "git-graph.graph.colours": [
        "#b80025",
        "#0a9cf5",
        "#af43be",
        "#00ff9f",
        "#f14c4c",
        "#11a8cd",
        "#23d18b",
        "#f7e26b"
      ],
      "git.autoRepositoryDetection": false,
      "git.autofetch": true,
      "git.confirmSync": false,
      "git.enableSmartCommit": true,
      "git.ignoreMissingGitWarning": true,
      "git.openRepositoryInParentFolders": "always",
      "git.repositoryScanMaxDepth": 0,
      "gitlens.views.commitDetails.files.layout": "list",
      "go.buildTags": "",
      "go.coverOnSave": false,
      "go.formatTool": "gofmt",
      "go.inferGopath": true,
      "go.lintOnSave": "workspace",
      "go.lintTool": "golint",
      "go.testOnSave": false,
      "go.toolsManagement.autoUpdate": true,
      "go.vetOnSave": "workspace",
      "go.buildFlags": [],
      "go.lintFlags": [],
      "go.vetFlags": [],
      "go.formatFlags": [],
      "go.testFlags": [
        "-v"
      ],
      "javascript.updateImportsOnFileMove.enabled": "always",
      "notebook.lineNumbers": "on",
      "notebook.output.textLineLimit": 50,
      "quokka.automaticRestart": true,
      "quokka.showStartViewOnFeatureRelease": false,
      "security.workspace.trust.untrustedFiles": "open",
      "terminal.integrated.allowChords": false,
      "terminal.integrated.cursorStyle": "underline",
      "terminal.integrated.cursorWidth": 3,
      "terminal.integrated.defaultProfile.linux": "zsh",
      "terminal.integrated.defaultProfile.windows": "PowerShell",
      "terminal.integrated.drawBoldTextInBrightColors": false,
      "terminal.integrated.fontFamily": "JetBrains Mono NL Medium",
      "terminal.integrated.fontSize": 14,
      "terminal.integrated.fontWeight": "400",
      "terminal.integrated.smoothScrolling": true,
      "thunder-client.autoRunCurl": true,
      "thunder-client.httpLibrary": "axios",
      "thunder-client.httpVersion": "HTTP/2",
      "typescript.updateImportsOnFileMove.enabled": "always",
      "update.enableWindowsBackgroundUpdates": false,
      "update.mode": "manual",
      "window.titleBarStyle": "custom",
      "workbench.iconTheme": "material-icon-theme",
      "workbench.panel.defaultLocation": "right",
      "workbench.startupEditor": "none",
      "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
      },
      "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
      },
      "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
      },
      "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[less]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "[python]": {
        "editor.formatOnType": true
      },
      "tabnine.experimentalAutoImports": true,
      "editor.minimap.renderCharacters": false,
      "editor.minimap.size": "fill",
      "githubPullRequests.queries": [
        {
          "label": "Waiting For My Review",
          "query": "is:open review-requested:${user}"
        },
        {
          "label": "Assigned To Me",
          "query": "is:open assignee:${user}"
        },
        {
          "label": "Created By Me",
          "query": "is:open author:${user}"
        }
      ],
      "githubPullRequests.pullBranch": "never",
      "editor.quickSuggestions": {
        "strings": true
      },
      "go.inlayHints.functionTypeParameters": true,
      "go.inlayHints.compositeLiteralFields": true,
      "go.inlayHints.rangeVariableTypes": true,
      "rust-analyzer.inlayHints.bindingModeHints.enable": true,
      "rust-analyzer.inlayHints.closureCaptureHints.enable": true,
      "rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
      "mdb.shell": "mongo",
      "workbench.editorLargeFileConfirmation": 5,
      "python.analysis.inlayHints.callArgumentNames": true,
      "python.analysis.inlayHints.functionReturnTypes": true,
      "python.analysis.inlayHints.pytestParameters": true,
      "emeraldwalk.runonsave": {
        "commands": [
          {
            "match": ".go",
            "cmd": ""
          }
        ]
      },
      "go.inlayHints.compositeLiteralTypes": true,
      "go.inlayHints.assignVariableTypes": true,
      "go.coverOnSingleTest": true,
      "go.coverageDecorator": {
        "type": "gutter",
        "coveredGutterStyle": "slashgreen",
        "uncoveredGutterStyle": "slashred"
      },
      "debug.toolBarLocation": "commandCenter",
      "window.zoomLevel": -1,
      "settingsSync.ignoredExtensions": [
        "chrmarti.regex"
      ],
      "sonarlint.pathToNodeExecutable": "/Users/krisadanuchyothin/.nvm/versions/node/v20.9.0/bin/node",
      "typescript.inlayHints.variableTypes.suppressWhenTypeMatchesName": false,
      "explorer.confirmDragAndDrop": false,
      "typescript.inlayHints.enumMemberValues.enabled": true,
      "typescript.inlayHints.parameterNames.enabled": "literals",
      "indentRainbow.indicatorStyle": "classic",
      "indentRainbow.colors": [
        "#F8F8F212",
        "#42E66C12",
        "#f4474712",
        "#86dbfd12"
      ],
      "cSpell.ignoreRegExpList": [
        "/[\\u0E00-\\u0E7F]/g"
      ],
      "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "CONFIRM",
        "XXX",
        "[ ]",
        "[x]"
      ],
      "workbench.activityBar.location": "top",
      "workbench.productIconTheme": "material-product-icons",
      "workbench.colorTheme": "Poison Serpent Syntax 2",
      "gitlens.ai.experimental.provider": "openai",
      "gitlens.ai.experimental.openai.model": "gpt-4-turbo-preview"
    },
    "themes": [
      {
        "label": "Poison Serpent",
        "uiTheme": "vs-dark",
        "path": "./themes/Serpent-color-theme.json"
      },
      {
        "label": "Poison Serpent Syntax 2",
        "uiTheme": "vs-dark",
        "path": "./themes/Serpent-color-theme-v2.json"
      }
    ]
  }
}
